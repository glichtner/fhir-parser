{% include "fhirtypes.py" with context %}
{%- for klass in classes %}
{% if klass.name not in ("Element", "Resource") %}
class {{ klass.name }}Type(AbstractType):
    __resource_type__ = "{{ klass.name }}"
{% endif -%}{% endfor %}
__all__ = [
    "Boolean",
    "String",
    "Base64Binary",
    "Code",
    "Id",
    "Decimal",
    "Integer",
    "Integer64",
    "UnsignedInt",
    "PositiveInt",
    "Uri",
    "Oid",
    "Uuid",
    "Canonical",
    "Url",
    "Markdown",
    "Xhtml",
    "Date",
    "DateTime",
    "Instant",
    "Time",
    "FHIRPrimitiveExtensionType",
    "ElementType",
    "ResourceType",
    {%- for klass in classes %}
	{% if klass.name not in ("Element", "Resource") %}
	"{{ klass.name }}Type",
	{% endif -%}{% endfor %}
]
